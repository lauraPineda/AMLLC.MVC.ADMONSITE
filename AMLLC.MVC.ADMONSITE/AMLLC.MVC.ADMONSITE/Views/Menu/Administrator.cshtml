
@{
    ViewBag.Title = "Administrator";
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.HeadIcon = "";
    ViewBag.HeadTitle = "MENU";
}

<div class="container">
    <div class="bg-gray-lighter without-padding col-lg-offset-3 col-lg-6">
        <form method="@ViewBag.HttpMethod" action="@ViewBag.Action">
            <div class="row">
                <div class="bg-primary text-white text-center col-xs-12">
                    <h2><i class="fa @ViewBag.HeadIcon"></i> @ViewBag.HeadTitle</h2>
                </div>
            </div>
            <div id="BodyContainer">
                <div class="row margin-TB-15">
                    <div class="col-xs-6">
                            <a href="/Evaluation/workers" class="btn btn-sq-lg btn-success">
                                <i class="fa fa-check fa-5x"></i><br />
                                EVALUATE<br>
                                WORKER
                            </a> 
                    </div>
                    <div class="col-xs-6">
                        <a href="#" class="btn btn-sq-lg btn-info">
                            <i class="fa fa-pie-chart fa-5x"></i><br />
                            GENERATE<br>REPORT
                        </a>  
                    </div>
                </div>
                <div class="row margin-TB-15">
                    <div class="col-xs-6">
                        <a href="#" class="btn btn-sq-lg btn-warning">
                            <i class="fa fa-plus fa-5x"></i><br />
                            ADD<br>WORKER
                        </a>
                    </div>
                    <div class="col-xs-6">
                        <a href="#" class="btn btn-sq-lg btn-danger">
                            <i class="fa fa-refresh fa-5x"></i><br />
                            UPDATE<br>WORKER
                        </a>
                    </div>
                </div>
            </div>
        </form>
       <div class="row footer-bottom">
            <div class="col-xs-12 col-lg-6">
                <a id="btnFootBack" href="/Login/Supervisor" class="btn btn-primary btn-block btn-lg">
                    <img src="data:image/svg+xml;utf8;base64," />
                    EXIT
                </a>
            </div>
        </div>
    </div>
</div>

