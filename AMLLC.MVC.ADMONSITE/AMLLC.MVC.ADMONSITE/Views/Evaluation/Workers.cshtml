@model AMLLC.MVC.ADMONSITE.Models.WorkersEvalutation

@{
    ViewBag.Title = "Workers";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>

<div class="container">
    <form method="GET" action="New">
        <div class="row">
            <div class="bg-brand text-white text-center col-xs-12">
                <h2><i class="fa fa-check-square-o"></i> EVALUATION</h2>
            </div>
        </div>
        <div class="container">
            @foreach (var item in Model.UserList)
            {
                <div id="Item-@item.IdUser" class="row margin-TB-2 bg-aquagray cursor-pointer">
                    <div class="bg-primary col-xs-3"><center><i class="fa fa-user fa-3x"></i></center></div>
                    <div><h3>&emsp;@item.UserName</h3></div>
                </div>
            }
        </div>
    </form>
    <div class="row">
        <div class="col-xs-12">
            <button class="btn btn-primary btn-block btn-lg">
                <img src="data:image/svg+xml;utf8;base64," />
                BACK
            </button>
        </div>
    </div>
</div>


<script>
    $(document).ready(function () {
        $(".cursor-pointer").on("click", function () {
            $("form").submit();
        });
    });
</script>
