@model AMLLC.MVC.ADMONSITE.Models.WorkersEvalutation

@{
    ViewBag.Title = "Workers";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container">
    <div class="bg-gray-lighter without-padding col-lg-offset-3 col-lg-6">
        <form method="GET" action="New">
            <div class="row">
                <div class="bg-primary text-white text-center col-xs-12">
                    <h2><i class="fa fa-check-square-o"></i> EVALUATION</h2>
                </div>
            </div>
            <div class="pre-scrollable scroll_x_none">
                @foreach (var item in Model.UserList)
            {
                    <div id="Item-@item.IdUser" class="bg-white row margin-TB-2 cursor-pointer">
                        <div class="bg-brand text-white col-xs-3"><center><i class="fa fa-user fa-3x"></i></center></div>
                        <div><label>&emsp;@item.UserName</label></div>
                    </div>
                }
            </div>
        </form>
        <div class="row footer-bottom">
            <div class="col-xs-12 col-lg-6">
                <button class="btn btn-primary btn-block btn-lg">
                    <img src="data:image/svg+xml;utf8;base64," />
                    BACK
                </button>
            </div>
        </div>
    </div>
</div>


<script>
    $(document).ready(function () {
        $(".cursor-pointer").on("click", function () {
            $("form").submit();
        });
    });
</script>
