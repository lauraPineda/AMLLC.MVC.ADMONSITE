@model AMLLC.MVC.ADMONSITE.Models.Assistance

@{
    ViewBag.Title = "Register";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<form method="POST">
    <div class="container">
        <div class="row">
            <div class="bg-primary text-center col-xs-12" id="HeaderWorker">
                <img src="data:image/svg+xml;utf8;base64," />

                <label name="sWorkerName" id="WorkerName">
                    @Model.sWorkerName
                </label>
            </div>

        </div>
        <div class="vertical-align">

                <button type="submit" class="proyecto">
                    @if (!Model.hasTodayAssisteance)
                    {
                        <img src="data:image/svg+xml;base64," class="imagen-proyecto"><!--alt sirve para que las imagenes seal indexadas correctamente en google -->
                        <h3>Click to register your assitance</h3>
                    }
                    else
                    {
                        <img src="data:image/svg+xml;base64," class="imagen-proyecto"/>
                        <h3>Click to register your depature time</h3>
                    }
                    </button>
        </div>
    </div>

</form>

